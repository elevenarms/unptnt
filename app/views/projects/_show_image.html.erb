<div id="image-image">
<% unless project.project_image_file_name.nil?  then %>
	  <%= image_tag project.project_image.url,:width=>'150' %>
<% else%>
	  <img src="/images/noimgprojectlarge.png">
<% end %> </div>
<div id="image-edit">
  <% if logged_in? && current_user.is_editor?(project) then %>
    <%= link_to_remote image_tag("/images/edit.png"), :url => edit_image_project_path(project), :method => :get %>
  <% end %>
</div>
