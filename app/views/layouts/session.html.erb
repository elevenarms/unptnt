<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
  <title>UnPtnt - Make Something!</title>
  <%= stylesheet_link_tag "base" -%>    
  <%= stylesheet_link_tag "blueprint/screen" -%>
  <%= javascript_include_tag "prototype" %>
  <%= javascript_include_tag "scriptaculous" %> 
  <%= javascript_include_tag "effects" %> 
  <%= javascript_include_tag "controls" %> 
  <%= javascript_include_tag :defaults %>
</head>
<body>
<div id="header" class="container">
	<div id="header-top" class="span-24">
		<div id="logo" class="prepend-1">
		  <img src="/images/unptntbug.png">
		</div>
		<div class="column prepend-3 span-3">
          	<%= link_to '<h1>UnPtnt</h1>', '/'%>
		</div>
		<div class="column span-2">
			<h4>projects</h4>
		</div>
		<div class="column span-2">
			<h4>dashboard</h4>
		</div>
		<div class="column span-2">
			<a href="http://blog.unptnt.com"><h4>blog</h4></a>
		</div>
		<div class="column prepend-7 span-1">
			<h4>login</h4>
		</div>
		<div class="column span-2">
			<h4>signup</h4>
		</div>
	</div>
	<div id="header-middle" class="span-24">
		<div id="tab-bar" class="column prepend-4">
				<ul id="tablist">
				<li class="tab-selected">
					<h4>overview</h4>
				</li>
				<li  class="tab">
					<h4><%= link_to 'people', related_users_project_path(@project) -%></h4>
				</li>
				<li  class="tab">
					<h4>docs</h4>
				</li>
				<li  class="tab">
				
				</li>			
			</ul>
		</div>
	</div>

</div>


  <div id="content" class="container ">
      <div id="layout" class="column span-24 ">
      <%=@content_for_layout%>
      </div>
    
  </div>
  <div id="footer" class="container">
    <hr>
    <div class="prepend-14 column span-6">
      <h3></h3>
    </div>
    <div class="column span-2">
      <h2></h2>
    </div>
  </div>  
</body>
</html>


